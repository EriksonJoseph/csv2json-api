{
    "version": 3,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "true",
        "PYTHONPATH": "/var/task"
    },
    "build": {
        "env": {
            "PYTHON_VERSION": "3.9",
            "PIP_CACHE_DIR": "/tmp/.pip-cache",
            "PIP_NO_CACHE_DIR": "false"
        }
    }
}